{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.3",
    "aws/aws-sdk-php-laravel": "~3.0",
    "barryvdh/laravel-dompdf": "^0.8.3",
    "barryvdh/laravel-ide-helper": "^2.5",
    "cweagans/composer-patches": "~1.0",
    "dnetix/redirection": "^0.4.5",
    "doctrine/dbal": "~2.8",
    "dompdf/dompdf": "~0.8",
    "fideloper/proxy": "^4.0",
    "filp/whoops": "~2.2",
    "google/cloud": "^0.132.0",
    "google/cloud-firestore": "^1.12",
    "guzzlehttp/guzzle": "^6.3",
    "guzzlehttp/psr7": "~1.4",
    "illuminate/support": "~5.6",
    "intervention/image": "~2.4",
    "jenssegers/mongodb": "^3.5",
    "kreait/firebase-php": "^5",
    "kreait/firebase-tokens": "^1.7",
    "laracasts/utilities": "~2.1",
    "laravel/framework": "5.6.*",
    "laravel/socialite": "~3.0",
    "laravel/tinker": "^1.0",
    "laravelcollective/html": "^5.4.0",
    "league/flysystem-aws-s3-v3": "^1.0",
    "maatwebsite/excel": "~2.1",
    "maxhoffmann/parsedown-laravel": "dev-master",
    "mcamara/laravel-localization": "^1.3",
    "mews/purifier": "~2.1.4",
    "milon/barcode": "^5.3",
    "modev/omnipay-payu": "dev-master",
    "mpociot/laravel-apidoc-generator": "^2.0",
    "nitmedia/wkhtml2pdf": "dev-master",
    "omnipay/common": "~3",
    "omnipay/dummy": "~3",
    "omnipay/paypal": "~3",
    "omnipay/stripe": "~3",
    "php-http/curl-client": "^1.7",
    "php-http/message": "^1.6",
    "platformsh/config-reader": "^1.0",
    "predis/predis": "^1.0",
    "romm1/omnipay-payu": "^3.0",
    "seatsio/seatsio-php": "^43.0",
    "spatie/icalendar-generator": "^1.0",
    "spatie/laravel-permission": "^2.28",
    "spatie/laravel-responsecache": "4.4.5",
    "spatie/laravel-tail": "^3.2",
    "superbalist/laravel-google-cloud-storage": "^2.0",
    "symfony/psr-http-message-bridge": "^2.0",
    "vinelab/http": "~1.5",
    "webup/laravel-sendinblue": "^1.1",
    "werneckbh/laravel-qr-code": "^0.1.3"
  },
  "require-dev": {
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^2.0",
    "phpspec/phpspec": "^6",
    "phpunit/phpunit": "^7.0",
    "symfony/css-selector": "~3.0",
    "symfony/dom-crawler": "~3.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories",
      "database",
      "app/Http/Controllers",
      "app/Models",
      "app/Attendize"
    ],
    "psr-4": {
      "App\\": "app/",
      "Attendize\\": "app/Models"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "enable-patching": true,
    "patches": {      
      "spatie/laravel-permission": {
        "Change eloquent to moloquent in spatie": "eviuspatches/spatieToMoloquent.patch"
      },
       "spatie/icalendar-generator": {
        "Agregando la letra Z al final de la fecha del evento": "eviuspatches/parchetimezone.patch"
      }
    },
    "laravel": {
      "dont-discover": []
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://cesarbarrios@bitbucket.org/modev/omnipay-payu.git"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
