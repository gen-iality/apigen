{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-dompdf": "^0.8.3",
        "cweagans/composer-patches": "~1.0",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "jenssegers/mongodb": "^3.4",
        "kreait/firebase-php": "^4.6",
        "kreait/firebase-tokens": "^1.7",
        "laracasts/utilities": "~2.1",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.4.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "morrislaptop/firestore-php": "^2.2",
        "mpociot/laravel-apidoc-generator": "^2.0",
        "nwidart/laravel-modules": "^4.0",
        "platformsh/config-reader": "^1.0",
        "predis/predis": "^1.0",
        "spatie/laravel-permission": "^2.28",
        "spatie/laravel-tail": "^3.2",
        "superbalist/laravel-google-cloud-storage": "^2.0",
        "webup/laravel-sendinblue": "^1.1",
        "werneckbh/laravel-qr-code": "^0.1.3"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "enable-patching": true,
        "patches": {
            "webup/laravel-sendinblue": {
                "Add event providerSendEmail into SendinBlueTransport": "patches/add-event-in-sendinblue.patch"  
            },
            "spatie/laravel-permission":{
                "Change eloquent to moloquent in spatie":"patches/spatieToMoloquent.patch"
            }    
        },
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
